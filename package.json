{
  "name": "gatsby-plugin-type-linter",
  "description": "Adds TSLint support to Gatsby",
  "version": "0.0.1",
  "author": "Trevor Harmon <thetrevorharmon@gmail.com>",
  "bugs": {
    "url": "https://github.com/tdharmon/gatsby-plugin-type-linter/issues"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "babel-plugin-remove-graphql-queries": "^2.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-preset-gatsby-package": "^0.1.2",
    "cross-env": "^5.1.4"
  },
  "homepage": "https://github.com/tdharmon/gatbsy-plugin-typescript-linter#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "typescript",
    "tslint",
    "linter"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "gatsby": ">2.0.0-alpha"
  },
  "repository": "https://github.com/tdharmon/gatbsy-plugin-typescript-linter",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  }
}
