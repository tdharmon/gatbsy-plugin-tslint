{
  "name": "gatsby-plugin-tslint",
  "description": "Adds TSLint support to Gatsby",
  "version": "0.0.3",
  "author": "Trevor Harmon <thetrevorharmon@gmail.com>",
  "bugs": {
    "url": "https://github.com/tdharmon/gatsby-plugin-tslint/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "cross-env": "^5.2.0"
  },
  "homepage": "https://github.com/tdharmon/gatbsy-plugin-tslint#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "typescript",
    "tslint",
    "linter"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "gatsby": ">2.0.0",
    "tslint": ">5.11.0",
    "tslint-loader": "^3.5.4"
  },
  "repository": "https://github.com/tdharmon/gatsby-plugin-tslint",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  }
}
